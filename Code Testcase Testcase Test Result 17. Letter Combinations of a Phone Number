class Solution:
    def letterCombinations(self, digits: str) -> List[str]:
        if not digits:
            return []

        phone_map = {
            "2": "abc",
            "3": "def",
            "4": "ghi",
            "5": "jkl",
            "6": "mno",
            "7": "pqrs",
            "8": "tuv",
            "9": "wxyz",
        }
        queue = [""]
        for digit in digits:
            letters = phone_map[digit]
            queue = [prev_combo + letter for prev_combo in queue for letter in letters]

        return queue
